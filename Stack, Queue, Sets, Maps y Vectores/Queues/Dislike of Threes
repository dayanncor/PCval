 int t;
    cin >> t;
    int n;
    
    while (t--) {
        queue<int> ar;
        int cont = 1;
        cin >> n;
        while (ar.size() < n) {
            if(cont%3 != 0 && cont%10 != 3){
                ar.push(cont);
            }
        cont++;
        }

        cout << ar.back() << endl;
    }
    }

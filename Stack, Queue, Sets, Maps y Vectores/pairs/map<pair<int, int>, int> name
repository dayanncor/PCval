#include <bits/stdc++.h>
using namespace std;

int main() {
    // Mapa con clave par<int, int> y valor int
    map<pair<int, int>, int> frecuencia;

    // Insertamos algunos pares de ejemplo
    frecuencia[{1, 2}]++;
    frecuencia[{1, 2}]++;
    frecuencia[{2, 3}]++;
    frecuencia[{3, 4}]++;
    frecuencia[{2, 3}]++;

    // Mostrar el contenido del mapa
    cout << "Frecuencia de pares:\n";
    for (auto& entrada : frecuencia) {
        auto par = entrada.first;
        int valor = entrada.second;
        cout << "(" << par.first << ", " << par.second << ") : " << valor << '\n';
    }

    return 0;
}

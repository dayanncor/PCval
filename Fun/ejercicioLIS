#include <bits/stdc++.h>
using namespace std;
#define forn(i,n) for(int i=0; i<n; i++)
#define endl '\n'
#define ll long long
#define all(x) x.begin(), x.end()
#define int int64_t
#define sz(x) x.size()
int lis(vector<int>& a) {
    vector<int>dp;
    int b = a.size();
    forn(i, b) {
        auto it = upper_bound(all(dp), a[i]);
        if (it != dp.end()) *it = a[i];
        else dp.push_back(a[i]);
    }
    return sz(dp);
}
main(){
ios_base::sync_with_stdio(false);
cin.tie(0);
cout.tie(0);
int n;
cin>>n;
vector<int>vec(n);
forn(i,n) cin>>vec[i];
cout<<lis(vec)<<endl;

 }

#include <bits/stdc++.h>
using namespace std;
#define forn(i,n) for(int i=0; i<n; i++)
#define endl '\n'
#define ll long long
#define int int64_t
main(){
ios_base::sync_with_stdio(false);
cin.tie(0);
cout.tie(0);
 int n;
 cin>>n;
 vector <int> vec(n);
forn(i,n) cin>>vec[i];
set<int>s;
int l=0,cont=0;
int maxi=0;
forn(i,n){
    while(s.count(vec[i])){
        s.erase(vec[l++]); 
    }
        s.insert(vec[i]); 
        cont=s.size();
     maxi= max(maxi, cont); 
    }
   cout<<maxi<<endl;
return 0; 
}
